{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"semi": 0,
		"comma-dangle": 0,
		"no-bitwise": 0,
		"no-useless-escape": 0,
		"no-param-reassign" : 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
	}
}
